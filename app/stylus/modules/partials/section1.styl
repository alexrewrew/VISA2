#section1-1
  background $blue
  position: relative;
  .fp-scroller
    overflow visible
  .section-first
    padding-top 80px
    +mobile()
      padding-top 20px

#section1-2
  background $yellow2
  position relative
  &:after
    pseudo(block, absolute)
    top 0
    left 0
    right 0
    height 50%
    background $blue

#section1-3
  .ornament
    bottom: -350px;
    right: -70px;

#section1-5
  .ornament
    top: -100px;
    left: 5px;

#section1-6
  .ornament
    bottom: -280px;
    right: 10px;

#section1-8
  .ornament
    bottom: -130px;
    right: 200px;

#section1-10
  .ornament
    top: -170px;
    left: -50px;

#section1-11
  .ornament
    bottom: -170px;
    left: 40%

#section1-12
  background $yellow2
  position relative
  &:after
    pseudo(block, absolute)
    bottom 0
    left 0
    right 0
    height 50%
    background $green
  +mobile()
    padding-top 50px

  .container-white
    padding-top 0
    padding-bottom 0
    .row
      padding-top 80px
      padding-bottom 96px
      +mobile()
        padding-top 40px
        padding-bottom 10px
      h3
        color $blue

#section1-13
  background $green
  h3
    text-align center
    color $white
    margin-top 107px
    +mobile()
      margin-top 50px
  .ornament.ornament3
    top: 350px
    left: -180px;
  .ornament.ornament4
    top: 750px
    right: -150px;
    +mobile()
      top: 1650px

  .container-white
    +mobile()
      padding-top 0

  .container-double-last
    background $blue
    padding-top 75px
    padding-bottom 75px
    margin-bottom 117px
    +mobile()
      padding-top 0
      margin-bottom 0
      padding-bottom 50px
    &:after
      height 50%
      top 0
      bottom auto
      background $green
    .container-white
      .row
        padding 140px 0
        +mobile()
          padding 50px 0
        .button
          margin 0 auto 44px
          max-width 360px

+iphone()
  #section1-1, #section1-2, #section1-3, #section1-4, #section1-5, #section1-6, #section1-7, #section1-8, #section1-9, #section1-10, #section1-11, #section1-12,, #section1-13
    .container
      padding-left 30px
      padding-right 30px
  #section1-1
    nav
      .container
        padding-left 15px
        padding-right 15px
  #section1-2, #section1-12, #section1-13
    .container-white
      .row
        padding-left 15px
        padding-right 15px

  #section1-13 .container-double-last .container-white .row
    padding: 50px 15px;

  .advantage-mobile
    left 20px





